{
  "name": "Chelmen",
  "description": "New tab with random quote with beautiful background",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Chelmen"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' https://www.google.com https://api.bing.com; object-src 'self'",
  "permissions": ["storage"]
}